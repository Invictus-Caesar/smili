.QWidget {
    background-color: beige;
}

QPushButton {
    background-color: palegoldenrod;
    border-color: darkkhaki;
    border-width: 2px;
    border-style: solid;
    border-radius: 5;
    padding: 3px;
    min-width: 9ex;
    min-height: 2.5ex;
}

QPushButton:hover {
    background-color: darkkhaki;
}

QPushButton:pressed {
    background-color: darkkhaki;
    padding-left: 5px;
    padding-top: 5px;
}

QLabel, QAbstractButton {
    font: bold;
}

QStatusBar QLabel {
    font: normal;
}

QListView {
    background-color: beige;
}

QLabel {
    border: 0px;
    padding: 0;
    background: none;
}

QLineEdit, QFrame {
    #background-color: #808080;
}

QLineEdit:focus, QFrame:focus {
    border-width: 3px;
    padding: 0px;
}

QToolTip {
    background-color: beige;
    border: 2px solid darkkhaki;
    padding: 5px;
    border-radius: 3px;
    opacity: 200;
}

QDialogButtonBox {
    button-layout: 0;
}

QMenuBar {
    background-color: ghostwhite;
}

QTabBar::tab {
    background: lightgray;
    color: black;
}

QTabBar::tab:selected {
    background: floralwhite;
    color: black;
}

QToolBar {
    background: ghostwhite;
}
